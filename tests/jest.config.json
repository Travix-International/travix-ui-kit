{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "builder/**/*.js",
    "!builder/program.js",
    "!builder/webpack.config.js",
    "components/**/*.js",
    "!components/index.js"
  ],
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "rootDir": "../",
  "snapshotSerializers": [
    "jest-serializer-enzyme"
  ],
  "transform": {
    ".*": "babel-jest"
  },
  "watchman": false
}
