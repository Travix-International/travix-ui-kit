{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "builder/**/*.js",
    "!builder/index.js",
    "!builder/webpack.config.js",
    "components/**/*.js",
    "!components/index.js"
  ],
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "snapshotSerializers": [
    "<rootDir>/node_modules/jest-serializer-enzyme"
  ],
  "transform": {
    ".*": "<rootDir>/node_modules/babel-jest"
  },
  "watchman": false
}
